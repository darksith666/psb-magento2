{
    "name": "paysbuy/module-magento2-gateway",
    "description": "Accept payment on your Magento2 website with PAYSBUY",
    "require": {
        "php": "~5.5.0|~5.6.0|~7.0.0",
        "magento/module-sales": "100.0.*",
        "magento/module-checkout": "100.0.*",
        "magento/module-payment": "100.0.*",
        "magento/module-quote": "100.0.*",
        "magento/framework": "100.0.*",
        "paysbuy/paysbuy-php": "*"
    },
    "type": "magento2-module",
    "version": "1.0.0",
    "license": [
        "OSL"
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://jon_paysbuy@bitbucket.org/psb_gateway/paysbuy-php.git"
        }
    ],
    "autoload": {
        "files": [ "registration.php" ],
        "psr-4": {
            "Paysbuy\\PsbGateway\\": ""
        }
    },
    "extra": {
        "map": [
            [
                "*",
                "Paysbuy/PsbGateway"
            ]
        ]
    }
}
